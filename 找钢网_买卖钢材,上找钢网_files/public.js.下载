(function(a){a.paramAjax=$.Base.Event.extend({init:function(b){$.extend(true,this,{url:null,type:"post",data:null,dataType:"json",async:false},b)}});a.http={ajax:function(f,d,b,c,e){$.ajax({url:f.url,type:f.type,data:f.data,dataType:f.dataType,async:f.async,beforeSend:function(){if(typeof(b)=="function"){b()}},success:function(g){if(typeof(d)=="function"){d(g)}},complete:function(){if(typeof(c)=="function"){c()}},error:function(g,i,h){if(typeof(e)=="function"){e(g,i,h)}}})},post:function(b,d,c){$.post(b,d,function(e){if(typeof(c)=="function"){c(e)}})},get:function(b,c){$.get(b,function(d){if(typeof(c)=="function"){c(d)}})},getJSON:function(b,d,c){$.getJSON(b,d,function(e){if(typeof(c)=="function"){c(e)}})},load:function(){},dataType:function(){return{xml:"xml",html:"html",script:"script",json:"json",jsonp:"jsonp",text:"text"}}};a.clearNoNum=function(b){(function(){b.value=b.value.replace(/[^\d.]/g,"");b.value=b.value.replace(/^\./g,"");b.value=b.value.replace(/\.{2,}/g,".");b.value=b.value.replace(".","$#$").replace(/\./g,"").replace("$#$",".").replace(/^(\-)*(\d+)\.(\d\d).*$/,"$1$2.$3")})()};String.prototype.format=function(c){var b=this;if(arguments.length>0){if(arguments.length==1&&typeof(c)=="object"){for(var e in c){if(c[e]!=undefined){var f=new RegExp("({"+e+"})","g");b=b.replace(f,c[e])}}}else{for(var d=0;d<arguments.length;d++){if(arguments[d]!=undefined){var f=new RegExp("({)"+d+"(})","g");b=b.replace(f,arguments[d])}}}}return b}})(window);