var zbxWxUtils={onMenuShareTimeline:function(){wx.onMenuShareTimeline({title:$("title").text(),link:window.location.href,imgUrl:$("#logoImg").attr("src"),success:function(){},cancel:function(){}})},onMenuShareAppMessage:function(){wx.onMenuShareAppMessage({title:$("title").text(),desc:"找钢网 国内成立最早的钢铁全产业链电商平台。",link:window.location.href,imgUrl:$("#logoImg").attr("src"),type:"link",dataUrl:"",success:function(){},cancel:function(){}})},setConfig:function(a){wx.config({debug:false,appId:a.appId,timestamp:a.timestamp,nonceStr:a.nonceStr,signature:a.signature,jsApiList:["onMenuShareTimeline","onMenuShareAppMessage"]});wx.ready(function(){zbxWxUtils.onMenuShareTimeline();zbxWxUtils.onMenuShareAppMessage()});wx.error(function(b){})},init:function(){var a=location.href.split("#")[0];$.ajax({type:"POST",dataType:"json",url:_base+"/wx/getWxInfo",data:{requestUrl:a},success:function(b){try{if(b.success){zbxWxUtils.setConfig(b.data)}}catch(c){}},error:function(){}})}};$(function(){zbxWxUtils.init()});